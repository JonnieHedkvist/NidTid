@model NidTid.WebUI.Models.ReportSummaryViewModel


<div class="container-fluid span11">
    <div class="span5">
        <div class="row-fluid">
            <fieldset>
            <legend>Rapporterad tid</legend>
            Idag:
                <div class="progress progress-success">
                    <div class="bar"  
                        style="width: 0%;">
                        @Model.TimeToday
                        timmar
                    </div>  
                </div>
                Denna månad:
                <div class="progress progress-info">
                    <div class="bar"  
                        style="width: 30%;">
                        @Model.TimeMonth
                        timmar
                    </div>  
                </div>
            </fieldset>
            </div>
        </div>
    </div>
