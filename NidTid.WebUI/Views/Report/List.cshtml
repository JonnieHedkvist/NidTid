@model IEnumerable<NidTid.Domain.Entities.Report>


        <div>
            <table class="table table-striped">
                <thead>
                     <tr>
                    <th>
                        Datum
                    </th>
                    <th>
                        Kund
                    </th>
                    <th>
                        Projekt
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Deb)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EjDeb)
                    </th>
                    <th>
                        Körsträcka
                    </th>
                    <th>
                        Notering
                    </th>
                    <th>
                        Användare
                    </th>

                    <th></th>
                </tr>
                </thead>
               

            @foreach (var item in Model) {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Date)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Project.Customer.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Project.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Deb)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EjDeb)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Km)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Notes)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.User.Name)
                    </td>

                    <td>
                        @Html.ActionLink("Ändra", "Edit", new { id=item.Id }) |
                        @Html.ActionLink("Radera", "Delete", new { id=item.Id })
                    </td>
                </tr>
            }

            </table>

        </div>
